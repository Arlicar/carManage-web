
.base_body {
	width: 100%;
	height: 100%;
	bottom: 0;
	z-index: 999;
}
.customCallout {
	box-sizing: border-box;
	background-color: #fff;
	border: 1px solid #ccc;
	border-radius: 30px;
	width: 150px;
	height: 40px;
	display: inline-flex;
	padding: 5px 20px;
	justify-content: center;
	align-items: center;
}
.content {
	flex: 0 1 auto;
	margin: 0 10px;
	font-size: 14px;
}

