<view class="content"><view class="top "><image class="topImg" src="/static/work.png"></image><text class="topText">派车申报</text></view><view class="main"><form><view class="bg-gray"><text class="main-text">申报人信息</text></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">姓名</text><text class="word" style="-webkit-flex:1;flex:1;">{{name}}</text></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">工号</text><text class="word" style="-webkit-flex:1;flex:1;">{{no}}</text></view><view class="bg-gray"><text class="main-text">申报信息</text></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">派车时间</text><picker class="word" style="-webkit-flex:1;flex:1;margin-top:24rpx;" mode="date" value="{{assignDate}}" data-event-opts="{{[['change',[['bindAssignDateChange',['$event']]]]]}}" bindchange="__e"><view>{{assignDate}}</view></picker></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">车牌号</text><picker class="input word" style="-webkit-flex:1;flex:1;" range="{{licenseArray}}" range-key="license" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><label>{{licenseArray[index].license}}</label></picker></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">勘察人姓名</text><picker class="input word" style="-webkit-flex:1;flex:1;" range="{{surveyArray}}" range-key="name" data-event-opts="{{[['change',[['bindsurveyPickerChange',['$event']]]]]}}" bindchange="__e"><label>{{surveyArray[surveyIndex].name}}</label></picker></view><view class="uni-flex card"><text class="word" style="-webkit-flex:1;flex:1;">地点</text><text data-event-opts="{{[['tap',[['choosePlace',['$event']]]]]}}" class="word" style="-webkit-flex:1;flex:1;font-weight:600;" bindtap="__e">{{place}}</text></view><block wx:if="{{placeDetail!==''}}"><view class="uni-flex card"><text style="width:100%;text-align:center;">{{"详细地址:"+placeDetail}}</text></view></block><view class="uni-flex card" style="height:180rpx;"><text class="word" style="-webkit-flex:1;flex:1;line-height:180rpx;">事故情况（选填）</text><textarea data-event-opts="{{[['input',[['__set_model',['','assignMsg','$event',[]]]]]]}}" class="main-textarea" value="{{assignMsg}}" bindinput="__e"></textarea></view><view class="bg-gray"><text class="main-text">审批人信息</text></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">审批人</text><picker class="input word" style="-webkit-flex:1;flex:1;" range="{{vetArray}}" range-key="name" data-event-opts="{{[['change',[['bindPickerChange2',['$event']]]]]}}" bindchange="__e"><label>{{vetArray[index2].name}}</label></picker></view><view><button class="btn" form-type="submit" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">提交</button></view></form></view></view>