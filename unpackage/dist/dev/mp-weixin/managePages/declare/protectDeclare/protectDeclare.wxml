<view class="content"><view class="top"><image class="topImg" src="/static/work.png"></image><text class="topText">保养申报</text></view><view class="main"><form><view class="bg-gray"><text class="main-text">申报人信息</text></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">姓名</text><text class="word" style="-webkit-flex:1;flex:1;">{{name}}</text></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">工号</text><text class="word" style="-webkit-flex:1;flex:1;">{{no}}</text></view><view class="bg-gray"><text class="main-text">申报信息</text></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">保养时间</text><picker class="word" style="-webkit-flex:1;flex:1;margin-top:16rpx;" mode="date" value="{{protectDate}}" data-event-opts="{{[['change',[['bindProtectDateChange',['$event']]]]]}}" bindchange="__e"><view>{{protectDate}}</view></picker></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">车牌号</text><picker class="input word" style="-webkit-flex:1;flex:1;" range="{{licenseArray}}" range-key="license" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><label>{{licenseArray[index].license}}</label></picker></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">保养详情</text><input class="word main-input" style="-webkit-flex:1;flex:1;" placeholder="请输入..." data-event-opts="{{[['input',[['__set_model',['','protectMsg','$event',[]]]]]]}}" value="{{protectMsg}}" bindinput="__e"/></view><view class="bg-gray"><text class="main-text">审批人信息</text></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">审批人</text><picker class="input word" style="-webkit-flex:1;flex:1;" range="{{vetArray}}" range-key="name" data-event-opts="{{[['change',[['bindPickerChange2',['$event']]]]]}}" bindchange="__e"><label>{{vetArray[index2].name}}</label></picker></view><view><button class="btn" form-type="submit" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">提交</button></view></form></view></view>