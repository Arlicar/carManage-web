<view class="content"><view class="top "><image class="topImg" src="/static/work.png"></image><text class="topText">录入还车信息</text></view><view class="main"><form><view class="bg-gray"><text class="main-text">归还信息</text></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">借车单</text><input class="word" style="-webkit-flex:1;flex:1;" placeholder="请输入借车单号" data-event-opts="{{[['input',[['__set_model',['$0','lendId','$event',[]],['formData']]]]]}}" value="{{formData.lendId}}" bindinput="__e"/></view><view class="uni-flex uni-column card" style="position:relative;"><text class="word" style="-webkit-flex:1;flex:1;width:50%;">照片上传</text><text style="position:absolute;right:130rpx;">{{tempFilePaths.length+"/1"}}</text></view><view class="uni-flex uni-row card" style="height:auto;"><view class="uni-uploader-body"><view class="uni-uploader__files"><block wx:for="{{tempFilePaths}}" wx:for-item="image" wx:for-index="index" wx:key="index"><block><view class="uni-uploader__file"><image class="uni-uploader__img" src="{{image}}" data-src="{{image}}" data-event-opts="{{[['tap',[['previewImage',['$event']]]]]}}" bindtap="__e"></image></view></block></block><view class="uni-uploader__input-box"><view data-event-opts="{{[['tap',[['chooseImage',['$event']]]]]}}" class="uni-uploader__input" bindtap="__e"></view></view></view></view></view><view class="uni-flex uni-column card" style="height:60rpx;width:80rpx;margin-top:0;margin-left:520rpx;"><text data-event-opts="{{[['tap',[['remove',['$event']]]]]}}" style="color:#DD524D;height:100%;width:100%;line-height:60rpx;font-weight:600;text-align:center;" bindtap="__e">清除</text></view><view class="bg-gray"><text class="main-text">借车信息</text></view><view class="uni-flex uni-row card"><text class="word" style="-webkit-flex:1;flex:1;">还车时间</text><picker class="word" style="-webkit-flex:1;flex:1;margin-top:18rpx;" mode="date" value="{{lendDate}}" data-event-opts="{{[['change',[['bindLendDateChange',['$event']]]]]}}" bindchange="__e"><view>{{lendDate}}</view></picker></view><view><button class="btn" form-type="submit" disabled="{{isAble}}" data-event-opts="{{[['tap',[['submit',['$event']]]]]}}" bindtap="__e">申请</button></view></form></view></view>